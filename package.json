{
  "name": "x-gateway-demo",
  "private": true,
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js . & npm run sass:watch",
    "sass": "sass --style=compressed src/public/stylesheets/main.scss public/styles/main.css",
    "sass:watch": "sass --watch src/public/stylesheets/main.scss public/styles/main.css",
    "prepare": "husky install",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pug": "^3.0.2",
    "sass": "^1.63.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@semantic-release/git": "^10.0.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-pug": "^1.2.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "semantic-release": "^21.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,pug}": [
      "eslint --fix"
    ]
  },
  "version": "1.0.0-dev.1"
}
